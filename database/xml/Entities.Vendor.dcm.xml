<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\Vendor" repository-class="Repositories\Vendor" table="vendor" tableName="vendor">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="true"/>
    <field name="shortname" type="string" length="255" nullable="true"/>
    <field name="nagios_name" type="string" length="255" nullable="true"/>
    <field name="bundle_name" type="string" length="255" nullable="true"/>
    <field name="created_at" type="datetime" nullable="true"/>
    <field name="updated_at" type="datetime" nullable="true"/>
    <one-to-many field="Switches" target-entity="Entities\Switcher" mapped-by="Vendor"/>
    <one-to-many field="consoleServers" target-entity="Entities\ConsoleServer" mapped-by="vendor"/>
  </entity>
</doctrine-mapping>
